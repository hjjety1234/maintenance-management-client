-- @brief root子节点创建完事件
function bodyBuildChildrenFinished(sprite)
    --@取根节点数据
    rootSprite = sprite
	--@图片存储数组格式确定
    imgdata = {pic1='',
       pic2='',
       pic3=''
       }       
end

-- @brief root节点消息方法
function bodyOnSpriteEvent(msg, param)
    if msg == MSG_ACTIVATE then -- 页面激活  
    UmsAgent:OnActivate(string.match(Alias.wjb_jizhanshebei, 'MODULE:\\(.*)'), "基站设备")
        --@取图片显示界面传过来的全局变量	
        local reg=Reg:create("PicReg")
		local type = Reg:getString(reg, "type")
		--@值为"yuantu"时，不做初始化操作
		if type=="yuantu2" then
		
		--@否则做初始化操作
		else   
		--@取从任务列表界面传过来的全局变量
		local regHandle = Reg:create("kemuDataRoom") 
		--@科目编码
        professionalCode = Reg:getString(regHandle, "kemuCode")
		--@科目名称
        retuenkemuName = Reg:getString(regHandle, "kemuName")
		--@任务编码
        taskCode = Reg:getString(regHandle, "taskNo")
		--@项目编码
        projectCode = Reg:getString(regHandle, "projectCode")
        Log:write("projectCode="..projectCode)
        Log:write("taskCode="..taskCode)
        Log:write("professionalCode="..professionalCode)
		--@专业编码
        retuenfieldCode = Reg:getString(regHandle, "fieldCode")
        --@专业名称
		retuenfieldName = Reg:getString(regHandle, "fieldName")
		--@找到根节点下的头部xuanze节点
        local xuanze = Sprite:findChild(rootSprite,"xuanze")
		--@找到xuanze节点下的title节点
        local title = Sprite:findChild(xuanze,"title")
		--@将科目名称对相应的title节点进行赋值
        Sprite:setProperty(title, "text", retuenkemuName) 
        --@自动执行录入按钮函数	
        latitude=Config:get('latitude')
        longitude=Config:get('longitude')
        address=Config:get('address')		
        luru()
        end			
    elseif msg == MSG_DEACTIVATE then
     UmsAgent:OnDeactivate()
    end
end

-- @brief 插件消息方法
function bodyOnPluginEvent(msg, param)
     if msg == 1000 then--用来显示经纬度
            local postDataString = Param:getString(param, 0)
            local postData = Json:loadString2Table(postDataString)
            if postData.latitude ~= nil then
                latitude=postData.latitude
            end
            if postData.longitude ~= nil then
                longitude=postData.longitude
            end
            if postData.longitude ~= nil and postData.latitude ~= nil then
                Map:getLocation(observer, 1001, tonumber(postData.latitude), tonumber(postData.longitude))  
            end
     end
     if msg == 1001 then--用来显示定位的位置
            local postDataString = Param:getString(param, 0)
