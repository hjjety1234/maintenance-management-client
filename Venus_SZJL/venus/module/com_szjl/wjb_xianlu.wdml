-- @brief root子节点创建完事件
function bodyBuildChildrenFinished(sprite)
    rootSprite = sprite
    imgdata = {pic1='',
       pic2='',
       pic3=''
       }      	   
end

-- @brief root节点消息方法
function bodyOnSpriteEvent(msg, param)
    if msg == MSG_ACTIVATE then -- 页面激活                 
        local reg=Reg:create("PicReg")
		local type = Reg:getString(reg, "type")
		if type=="yuantu" then		    
		else   
		    local regHandle = Reg:create("kemuDataRoom") 
            professionalCode = Reg:getString(regHandle, "kemuCode")
            retuenkemuName = Reg:getString(regHandle, "kemuName")
            taskCode = Reg:getString(regHandle, "taskNo")
            projectCode = Reg:getString(regHandle, "projectCode")
            Log:write("projectCode="..projectCode)
            Log:write("taskCode="..taskCode)
            Log:write("professionalCode="..professionalCode)
            retuenfieldCode = Reg:getString(regHandle, "fieldCode")
            retuenfieldName = Reg:getString(regHandle, "fieldName")
            local xuanze = Sprite:findChild(rootSprite,"xuanze")
            local title = Sprite:findChild(xuanze,"title")
            Sprite:setProperty(title, "text", retuenkemuName) 
            latitude=Config:get('latitude')
            longitude=Config:get('longitude')
            address=Config:get('address')			
            luru()
        end	      	
    elseif msg == MSG_DEACTIVATE then
    end
end

-- @brief 插件消息方法
function bodyOnPluginEvent(msg, param)                   
    if msg == 101 then
        jsonDecodeData = Http:jsonDecode('jinchangcailiaomingcheng')
        if (jsonDecodeData == nil or jsonDecodeData["Total"] == nil or jsonDecodeData["Total"] == '' or jsonDecodeData["Total"] == '0' or jsonDecodeData["Total"] == 0) then
            Dialog:show("提示", "无进场材料名称", "ok")           
            return
        else           
            len = jsonDecodeData["Total"]          
            local list = Sprite:findChild(rootSprite, 'listViewmingcheng')            
            ListView:removeAllItems(list, true) 
            ListView:loadItem(list, Sprite:findChild(rootSprite, 'mingchenglistitem'), len, 'mingchengloadListItem')
            ListView:adjust(list)
            setAllShoworHide(Sprite:findChild(rootSprite, 'cailiaomingchenglistShowNode'), 1)
        end
    end
    if msg == 102 then
        jsonDecodeData = Http:jsonDecode('jinchangcailiaoleixing')
        if (jsonDecodeData == nil or jsonDecodeData["Total"] == nil or jsonDecodeData["Total"] == '' or jsonDecodeData["Total"] == '0' or jsonDecodeData["Total"] == 0) then
            Dialog:show("提示", "无进场材料类型", "ok")           
            return
        else           
            len = jsonDecodeData["Total"]          
            local list = Sprite:findChild(rootSprite, 'listViewleixing')            
            ListView:removeAllItems(list, true) 
            ListView:loadItem(list, Sprite:findChild(rootSprite, 'leixinglistitem'), len, 'leixingloadListItem')
            ListView:adjust(list)
            setAllShoworHide(Sprite:findChild(rootSprite, 'cailiaoleixinglistShowNode'), 1)
        end
    end 
    if msg == 103 then
        jsonDecodeData = Http:jsonDecode('jinchangcailiaocankao')          
        if (jsonDecodeData == nil or jsonDecodeData["Total"] == nil or jsonDecodeData["Total"] == '' or jsonDecodeData["Total"] == '0' or jsonDecodeData["Total"] == 0) then
            Dialog:show("提示", "无进场材料参考标准", "ok")           
            return
        elseif (jsonDecodeData["Total"] == 1 or jsonDecodeData["Total"] == "1") then                                
            len = jsonDecodeData["Total"]                  
            local list = Sprite:findChild(rootSprite, 'listViewcankaobiaozhun')            
