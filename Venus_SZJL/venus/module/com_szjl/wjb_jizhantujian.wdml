function editOnTextChanged(sprite)
       local txt= Sprite:getProperty(sprite, 'text')
       if txt=='' or txt == nil then 
            Sprite:setProperty(sprite, 'color', '#c5c6c5')
           Sprite:setProperty(sprite, 'text', '请输入备注信息')
       end
end

