	   local tijiaobiaozhi = Sprite:findChild(jiluchaxunitemBtn, 'tijiaobiaozhi')
       local supervisionresultId = Sprite:findChild(jiluchaxunitemBtn, 'supervisionresultId') 
       if (jsonDecodeData ~= nil) then
             local value =  jsonDecodeData['Value'][index]
             if value.supervision_time~=nil then
			     local date = Split( value.supervision_time,'-')
                 Sprite:setProperty(jiluchaxuntime, 'text', date[1].."年"..date[2].."月"..date[3].."日")                  
             end
             if value.supervision_result~=nil then
			     if value.supervision_result=="0" then
                     Sprite:setProperty(hegebiaozhi, 'text', "合格")
                 elseif value.supervision_result=="1" then	
                     Sprite:setProperty(hegebiaozhi, 'text', "不合格")
                 end					 
             end    
             if value.state~=nil then
                 if value.state=="1" then
                     Sprite:setProperty(tijiaobiaozhi, 'text', "已提交")
                 elseif value.state=="0" then	
                     Sprite:setProperty(tijiaobiaozhi, 'text', "未提交")
                 end	
             end
             if value.supervision_result_id~=nil then
                Sprite:setProperty(supervisionresultId, 'text', value.supervision_result_id)
             end				
	   end			 
end

