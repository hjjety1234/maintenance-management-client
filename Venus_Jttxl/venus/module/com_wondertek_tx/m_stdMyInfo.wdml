-- @jldu 查询用户信息
function queryUserInfo()
    if(employeeId == '0') then
        retUserTable = {}
        local persionalInfoHandler = Reg:create("personalInfo")
        Log:write('table is ',persionalInfoHandler)
        local employeeNameH = Reg:getString(persionalInfoHandler, 'employeeName')
        local employeePhone = Reg:getString(persionalInfoHandler,'employeePhone')
        local contact = {}
        contact[1] = ''
        contact[2] = 0
        contact[3] = employeeNameH
        contact[4] = '本机通讯录'
        contact[5] = ''
        contact[6] = ''
        contact[7] = ''
        contact[8] = employeePhone
        for i=9,28 do 
            contact[i] = ''
        end  
        Log:write("@jldu 本机通讯录表格信息contact = ",contact)
        table.insert(retUserTable, contact)
    else
        local sql = "SELECT emp.user_company_id,"..
                "emp.manage_flag,"..
                "emp.employee_name,"..        
                "com.company_name,"..              
                "emp.headship_name,"..  
                "emp.department_name,"..
                "emp.mood,"..
                "emp.mobile,"..
                "emp.mobile_short,"..
                "emp.tel,"..
                "emp.tel_short,".. 
                "emp.home_telephone,"..          
                "emp.email,"..
                "emp.qq,"..
                "emp.weibo,"..
                "emp.weixin,"
        if tonumber(orgFlag) == 1 then
            sql = sql .. "com.company_name,".. 
                "dept.department_name,"
        else
            sql = sql .. "emp.user_company,"..
                "emp.department_name,"
        end
        sql = sql .. "emp.headship_name,".. 
                "emp.company_address,"..
                "emp.school,"..
                "emp.user_major,"..
                "emp.user_grade,"..
                "emp.user_class,"..
                "emp.student_id,"..
                "emp.native_place,"..
                "emp.home_address,"..
                "emp.remark"
        if tonumber(orgFlag) == 1 then
            sql = sql .. " FROM tb_c_employee emp,tb_c_department dept,tb_c_company com"..
                " WHERE emp.department_id = dept.department_id"..
                " AND emp.company_id = com.company_id"..
                " AND emp.employee_id = '"..Config:get("employeeId").."'"..
                " AND emp.company_id = '"..Sprite:getText(selectCompanyId).."';"
        else
            sql = sql .. " FROM tb_c_employee emp,tb_c_company com"..
                " WHERE emp.company_id = com.company_id"..  
                " AND emp.employee_id = '"..Config:get("employeeId").."'"..
                " AND emp.company_id = '"..Sprite:getText(selectCompanyId).."';"
        end
                
        Log:write("查询用户信息SQL = ",sql)
        bRet, retUserTable, errMsg = Sqlite:query(databaseName, sql)
    end
    Log:write("用户详细信息JSON数据 retUserTable = ", retUserTable)
    loadUserData()  
end

-- @jldu 加载用户数据
function loadUserData()
    local adminNode = Sprite:findChild(rootSprite,'adminNode')
    if tonumber(retUserTable[1][2]) == 0 then
        setAllShoworHide(adminNode,0)
    end 
    for i=1,#fieldTipTable do
        local fieldNode = Sprite:findChild(rootSprite,fieldTipTable[i])
        Sprite:setProperty(fieldNode,'text',retUserTable[1][i+2])
    end
end

-- @jldu 拨打电话
function telOnSelect(sprite)
    local data = Sprite:getData(sprite)
    Log:write("@jldu the data of the node is "..data)
    local mobile
    local listitem = Sprite:getParent(sprite)
    if data == "1" then      
        mobile = Sprite:getText(Sprite:findChild(listitem, "mobile"))
        
    elseif data == "2" then
        mobile = Sprite:getText(Sprite:findChild(listitem, "mobileShort"))
    elseif data == "3" then
        mobile = Sprite:getText(Sprite:findChild(listitem, "telephone"))
    elseif data == "4" then
        mobile = Sprite:getText(Sprite:findChild(listitem, "telShort"))
    elseif data == "5" then 
        mobile = Sprite:getText(Sprite:findChild(listitem, "telFamily"))
    end
    Log:write("@jldu mobile is "..mobile)
    if mobile ~= nil and mobile ~= '' then
        if Util:openCallDialog('callFinished',mobile, 0) then
            Log:write("当data为 "..data.." 时，tel succes!")
        else
            Dialog:show('提示', '调用接口失败，请检查手机设置！', 'ok')
        end
    else
        Dialog:show('提示', '电话号码为空！', 'ok')
    end     
end

