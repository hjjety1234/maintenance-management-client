    end 
    if Sprite:getText(telFamily) == '' then
        ListView:removeItem(listview1, Sprite:getParent(telFamily), 1)
    end 

    -- 网络信息
    local email = Sprite:findChild(rootSprite,'email')
    local QQ = Sprite:findChild(rootSprite,'QQ')
    local microblog = Sprite:findChild(rootSprite,'microblog')
    local microMessage = Sprite:findChild(rootSprite,'microMessage')
