    -- 学校信息
    local school = Sprite:findChild(rootSprite,'school')
    local major = Sprite:findChild(rootSprite,'major')
    local grade = Sprite:findChild(rootSprite,'grade')
    local class = Sprite:findChild(rootSprite,'class')
