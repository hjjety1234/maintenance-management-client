        ListView:removeItem(listview4, Sprite:getParent(major), 1)
    end 
    if Sprite:getText(grade) == '' then
        ListView:removeItem(listview4, Sprite:getParent(grade), 1)
    end 
    if Sprite:getText(class) == '' then
        ListView:removeItem(listview4, Sprite:getParent(class), 1)
    end 
    if Sprite:getText(studentId) == '' then
        ListView:removeItem(listview4, Sprite:getParent(studentId), 1)
    end 

    -- 个人信息 
    local nativePlace = Sprite:findChild(rootSprite,'nativePlace')
    local homeAddr = Sprite:findChild(rootSprite,'homeAddr')
    local mark = Sprite:findChild(rootSprite,'mark')
    if Sprite:getText(nativePlace) == '' then
        ListView:removeItem(listview5, Sprite:getParent(nativePlace), 1)
    end 
    if Sprite:getText(homeAddr) == '' then
        ListView:removeItem(listview5, Sprite:getParent(homeAddr), 1)
    end 
    if Sprite:getText(mark) == '' then
        ListView:removeItem(listview5, Sprite:getParent(mark), 1)
    end 

    -- 调整界面布局
    adjustListView()
end

-- listView布局重新调整
function adjustListView()
    Log:write("@jldu 进入调整布局函数")
    for i=1,5 do 
            local listviewStr = 'listview'..i
            local itemImgStr = 'itemImg'..i
            local itemNodeStr = 'itemNode'..i
            listviewNode = Sprite:findChild(rootSprite, listviewStr)
            itemImgNode = Sprite:findChild(rootSprite, itemImgStr)
            itemNode = Sprite:findChild(rootSprite, itemNodeStr)
        
            Log:write("listviewStr = "..listviewStr.."; itemImgStr = "..itemImgStr.."; itemNodeStr = "..itemNodeStr)
            
            itemCount = tonumber(ListView:getItemCount(listviewNode))
            Log:write("第"..i.."个listItem中列表项个数",itemCount)

            if itemCount == 1 then
                Sprite:setProperty(itemImgNode,'src','file://image/jttxl/yihang.png')
            elseif itemCount == 2 then
                Sprite:setProperty(itemImgNode,'src','file://image/jttxl/lianghang.png')
            elseif itemCount == 3 then
                Sprite:setProperty(itemImgNode,'src','file://image/jttxl/sanhang.png')
            elseif itemCount == 4 then
                Sprite:setProperty(itemImgNode,'src','file://image/jttxl/sihang.png')
            elseif itemCount == 5 then
                Sprite:setProperty(itemImgNode,'src','file://image/jttxl/wuhang.png')
            end 

            local height1 = itemCount*63.4 + 50        
            Log:write("@jldu height1 = ",height1)
            if itemCount == 0 then
                ListView:removeItem(listview,itemNode,1)
            else 
                Sprite:setProperty(itemImgNode,'rect',"0,35,458,"..itemCount*63.4)
                Sprite:setProperty(itemNode,'rect',"10,0,458,"..height1)
                Sprite:setProperty(listviewNode,'rect',"0,35,458,"..itemCount*63.4)
            end
        end 
        ListView:adjust(listview) 
end


