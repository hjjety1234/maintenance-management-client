    end 
    if Sprite:getText(QQ) == '' then
        ListView:removeItem(listview2, Sprite:getParent(QQ), 1)
    end 
    if Sprite:getText(microblog) == '' then
        ListView:removeItem(listview2, Sprite:getParent(microblog), 1)
    end
    if Sprite:getText(microMessage) == '' then
        ListView:removeItem(listview2, Sprite:getParent(microMessage), 1)
    end 

    -- 单位信息
    local company = Sprite:findChild(rootSprite,'company')
    local deptDisplay = Sprite:findChild(rootSprite,'deptDisplay') 
    local heashipDisplay = Sprite:findChild(rootSprite,'heashipDisplay')
    local companyAddr = Sprite:findChild(rootSprite,'companyAddr')
    if Sprite:getText(company) == '' then
        ListView:removeItem(listview3, Sprite:getParent(company), 1)
    end 
    if Sprite:getText(deptDisplay) == '' then
        ListView:removeItem(listview3, Sprite:getParent(deptDisplay), 1)
    end
    if Sprite:getText(heashipDisplay) == '' then
        ListView:removeItem(listview3, Sprite:getParent(heashipDisplay), 1)
    end
    if Sprite:getText(companyAddr) == '' then
        ListView:removeItem(listview3, Sprite:getParent(companyAddr), 1)
