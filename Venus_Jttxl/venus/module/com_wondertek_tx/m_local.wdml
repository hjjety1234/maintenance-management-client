-- @brief root子节点创建完事件
function bodyBuildChildrenFinished(sprite)
    rootSprite = sprite    
    list = Sprite:findChild(rootSprite, 'sampleList')
    fenleiList = Sprite:findChild(rootSprite, 'fenleiList')
    phoneList = Sprite:findChild(rootSprite, 'phoneList')
    resetBtn = Sprite:findChild(rootSprite, 'resetBtn')
    --更多下拉框
    comboboxListview = Sprite:findChild(rootSprite, 'comboboxListview')
    comboboxItem = Sprite:findChild(rootSprite, 'comboboxItem')
    blank = Sprite:findChild(rootSprite, 'blank')
    comboboxsNode = Sprite:findChild(rootSprite, 'comboboxsNode')
    reg = Reg:create('group_data')
    company_name = Reg:getString(reg, 'company_name')
    Log:write('###company_name', company_name)
end

-- @brief root节点消息方法
function bodyOnSpriteEvent(msg, param)
    if msg == MSG_ACTIVATE then -- 页面激活 
        setAllShoworHide(Sprite:findChild(rootSprite, 'fenleiListNode'), 0)
        Sprite:setVisible(blank, 0)
        Sprite:setEnable(blank, 0)
        Sprite:setActive(blank, 0)
        local newNode = Sprite:findChild(rootSprite, 'newNode')
        if (Config:get("isNeedUpdate") == "true") then 
            setAllShoworHide(newNode, 1)
        else 
            setAllShoworHide(newNode, 0)
        end 
        Sprite:setProperty(Sprite:findChild(rootSprite,'personBtn'), 'src', 
            'file://image/book/person_d.png')
        Sprite:setProperty(Sprite:findChild(rootSprite,'searchBtn'), 'src',
            'file://image/book/lianxiren_d.png')
        Sprite:setProperty(Sprite:findChild(rootSprite,'contactBtn'), 'src', 
            'file://image/book/zuijinglianxiren_s.png')
        Sprite:setProperty(Sprite:findChild(rootSprite,'siteBtn'), 'src', 
            'file://image/book/site_d.png')
        getContacts()
        Timer:set(222, 100, "checkDeviceInfo")
         --更改标题
        queryGroupData()
        showTitleData(company_name)
    elseif msg == MSG_DEACTIVATE then
    elseif msg == MSG_CONTACTS then
        contactsTable = getContactsResp()
        groupContacts = contactsTable
        showContactList(contactsTable)
    elseif msg == MSG_CONTACTSGROUP then 
        groupTable = getContactsGroupResp()
        showGroupList()
    elseif msg == MSG_EACHCONTACTSGROUPINFO then 
        groupContacts = getEachContactsGroupInfoResp()
        showContactList(groupContacts)
    end
end

-- @brief 插件消息方法
function bodyOnPluginEvent(msg)
    if msg == 101 then
    elseif msg == 1000 then 
       checkDeviceRespProc()
    elseif msg == 101 + MSG_CACHEDATA_RELOAD then -- 101请求的数据有更新
    elseif msg == MSG_NETWORK_ERROR then -- 拨号失败
        Log:write('拨号失败')
    elseif msg > MSG_NETWORK_ERROR then -- 请求超时
        Log:write('请求超时')
    end
end

-- @brief 按键响应处理
function mainNodeOnKeyUp(sprite, kc)
    if kc == Key.F2 then -- 按下返回键
        Scene:back()
        return 1
    end
end

---------------------------------------util functions---------------------------
-- @brief 显示联系人列表
function showContactList(tbl)
    clearLastConstactStatus()
    buildContactList(tbl)
end

-- @brief 清空联系人列表
function clearLastConstactStatus()
    ListView:removeAllItems(list, 1, 1)
end

-- @brief 加载联系人列表
function buildContactList(tbl)
    displayTable = tbl
    ListView:loadItem(list, Sprite:findChild(rootSprite, 'listItem'), 
        #displayTable, 'loadItemData')
    ListView:adjust(list)
end

-- @brief 加载单个联系人信息
function loadItemData(list, item, index)
    Sprite:setProperty(item, 'extendstyle', '1111')
    Sprite:setRect(item, 0, 0, 430, 85) 
    local name = Sprite:findChild(item, 'name')
    Sprite:setProperty(name, 'text', displayTable[index+1].name)
    local phone = Sprite:findChild(item, 'phone')
    Sprite:setProperty(phone, 'text', displayTable[index+1].phone[1])
    local phoneindex = Sprite:findChild(item, 'phoneindex')
    Sprite:setProperty(phoneindex, 'text', index+1)
    local phoneall = displayTable[index+1].phone
    if #phoneall > 1 then
        local manyphone = Sprite:findChild(item, 'manyphone')
        Sprite:setVisible(manyphone, 1)
    end
end

-- @brief 点击群组按钮，查询本机通讯录群组列表
function fenleiBtnOnselect(sprite)
    if Sprite:isVisible(Sprite:findChild(rootSprite, 'fenleiListNode'))== 1 then
        setAllShoworHide(Sprite:findChild(rootSprite, 'fenleiListNode'), 0)
        Sprite:setVisible(blank, 0)
        Sprite:setEnable(blank, 0)
        Sprite:setActive(blank, 0)
    else
        getContactsGroup()
    end
end

-- @brief 显示本机通讯录群组列表
function showGroupList()
    local fenleiNum = #groupTable
    ListView:removeAllItems(fenleiList, true) 
    ListView:loadItem(fenleiList, Sprite:findChild(rootSprite, 'fenleiItem'), 
        fenleiNum, 'loadfenleiData')        
    Sprite:setRect(Sprite:findChild(rootSprite, 'fenleiListNode'), 
        160, 60, 230, 25 + 60 * fenleiNum + 25)
    Sprite:setRect(fenleiList, 0, 22, 230, 60 * fenleiNum) 
    Sprite:setRect(Sprite:findChild(rootSprite, 'fenleiImg'), 
        0, 12, 230, 60 * fenleiNum + 25)
    ListView:adjust(fenleiList)
    setAllShoworHide(Sprite:findChild(rootSprite, 'fenleiListNode'), 1)
    Sprite:setVisible(blank, 1)
    Sprite:setEnable(blank, 1)
    Sprite:setActive(blank, 1)
end

-- @brief 加载单个群组信息
function loadfenleiData(list, item, index)
    Sprite:setProperty(item, 'extendstyle', '1111')
    Sprite:setRect(item, 0,0,230,60)   
    local name = Sprite:findChild(item, 'name')
    Sprite:setProperty(name, 'text', groupTable[index+1].title)
    Sprite:setProperty(item, "data", groupTable[index+1].id)
end

-- @brief 显示某个群组下的所有联系人
function fenleiitemOnSelect(sprite)
    setAllShoworHide(Sprite:findChild(rootSprite, 'fenleiListNode'), 0)
    Sprite:setVisible(blank, 0)
    Sprite:setEnable(blank, 0)
    Sprite:setActive(blank, 0)
    local groupName = Sprite:getText(Sprite:findChild(sprite, 'name'))
    local groupId = Sprite:getData(Sprite:getParent(sprite))
    Log:write("被选择的群组ID为:"..groupId)
    Sprite:setProperty(Sprite:findChild(rootSprite, 'fenleibtnName'), 'text', groupName..'▼')
    ListView:removeAllItems(list, true) 
    local oneGroup = {}
    if groupName == '全部' then
        groupContacts = contactsTable
        showContactList(contactsTable)
    else
        getEachContactsGroupInfo(groupId)
    end
end

--@brief 当搜索框本文发生变化的消息处理
function editOnTextChanged(sprite) 
    setAllShoworHide(resetBtn,1)
    Log:write("文本发生改变时触发") 
    local editeKeyword = string.gsub(string.lower(Sprite:getProperty(sprite, 'text')), "%s", "")
    Log:write("搜索关键字为", editeKeyword)
    ListView:removeAllItems(list, true) 
    local tempTab = {}
    if editeKeyword ~= '' then    
        for i=1 , #groupContacts do
            if string.find(groupContacts[i].name, editeKeyword) ~= nil 
                or string.find(firstSpell(groupContacts[i].sortKey), editeKeyword) ~= nil
                or string.find(groupContacts[i].phone[1], editeKeyword) ~= nil  then
                table.insert(tempTab, groupContacts[i])
            end
        end
        showContactList(tempTab)
    else
        showContactList(groupContacts)
    end
end 

-- @brief 获取首字母
function firstSpell(sortKey)
    sortKey = string.gsub(sortKey, "[^a-zA-Z%s]", "")
    local sortKeyArray = Util:split(sortKey, " ")
    local fs = ""
    for i = 1, #sortKeyArray do 
        fs = fs..sortKeyArray[i]
    end 
    --Log:write('fs key is ',fs)
    return fs
end

-- @brief 搜索框获得焦点时
function editOnSetFocus(sprite)
